{% endblock %}
{% block breadcrumb %}

    <div class="row wrapper border-bottom white-bg page-heading">
        <div class="col-lg-10">
            <h1>Modifier un prospect</h1>
            <ol class="breadcrumb">
                <li class="breadcrumb-item">
                    <a href="{{ path('home') }}">Accueil</a>
                </li>
                <li class="breadcrumb-item">
                    <a href="{{ path('prospect_list') }}">Prospect</a>
                </li>
                <li class="breadcrumb-item active">
                    <strong>Modifier un prospect</strong>
                </li>
            </ol>
        </div>
    </div>
{% endblock %}

{% block content %}
    <div class="row">
        {{ include('prospect/_form.html.twig') }}
    </div>

    <a href="{{ path('prospect_list') }}">Retourner Ã  la liste des prospect</a>
{% endblock %}

{% block javascript_script %}
