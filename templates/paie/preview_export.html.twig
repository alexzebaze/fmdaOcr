<div class="row">
    <div class="col-lg-4">
        <div class="ibox">
            <div class="ibox-content">
                {{ include('paie/_form.html.twig') }}
            </div>
        </div>
    </div>
    <div class="col-lg-8">
        <div class="ibox">
            <div class="ibox-content">
            {% if fichePaie.getDocumentFile %}
                {% if fichePaie.getDocumentFile|slice(-3,3)|lower == "pdf" %}
                    <object class="pdfupload" data="/uploads/paies/{{fichePaie.getDocumentFile}}" type="application/pdf" width="100%" height="980px">lien document : <a href="/uploads/paies/{{fichePaie.getDocumentFile}}">{{ fichePaie.getDocumentFile }}</a></object>
                    {% else %}
                    <img src="/uploads/paies/{{fichePaie.getDocumentFile}}" alt="fichier facture uploadÃ©" width="100%">
                {% endif %}
            {% else %}
                <span class="text-muted">-- AUCUN FICHIER EXPORTE --</span>
            {% endif %}
            </div>
        </div>
    </div>         
</div>
