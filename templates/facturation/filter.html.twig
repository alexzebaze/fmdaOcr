{{ form_start(form, { 'attr': {'id': 'fact_filter'} }) }}
<div class="row">
    <div class="col-lg-2 form-group">
        {{ form_row(form.mois) }}
    </div>
    <div class="col-lg-1 form-group">
        {{ form_row(form.annee) }}
    </div>
    <div class="col-lg-2 form-group">
        {{ form_row(form.chantier) }}
    </div>
    <div class="col-lg-2 form-group">
        {{ form_row(form.fournisseur) }}
    </div>
    <div class="col-lg-2 form-group">
        {{ form_row(form.categorie_lot) }}
    </div>
    <div class="col-lg-2 form-group">
        {{ form_row(form.lot) }}
    </div>
    <div class="col-lg-2 form-group">
        {{ form_row(form.is_paid) }}
    </div>
    <div class="col-lg-2 form-group">
        <button class="btn btn-primary" style="margin-top: 21px">{{ button_label|default('Appliquer') }}</button>
        {#<a class="btn btn-primary" style="margin-top: 21px;margin-left: 5px;" href="{{path('facturation_list')}}">tout</a>#}
    </div>
    
</div>
{{ form_end(form) }}
