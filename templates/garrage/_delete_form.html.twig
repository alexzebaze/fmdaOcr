<form method="post" action="{{ path('garrage_delete', {'id': garrage.id}) }}" onsubmit="return confirm('Are you sure you want to delete this item?');">
    <input type="hidden" name="_method" value="DELETE">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ garrage.id) }}">
    <button class="btn">Delete</button>
</form>
